{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/(.*)",
      "dest": "/index.js",
      "methods" : ["GET","POST","OPTIONS"],
      "headers" : {
        "Access-Control-Allow-Origin" : "*"
      }
    }
  ]
}